{
  "name": "nestjs-generator",
  "version": "0.1.0",
  "description": "Generator for Nest.js",
  "type": "module",
  "keywords": [
    "nest.js",
    "node.js",
    "scaffold",
    "generator",
    "boilerplate"
  ],
  "bin": {
    "nestjs-generator": "dist/index.js"
  },
  "main": "index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -a -u 1 \"src/generators/**/generator/templates/**/*\" dist/src/",
    "build": "npm run clean && tsc && tsc-alias && npm run copy-files",
    "generate": "node dist/index.js"
  },
  "author": "Vyacheslav Diakov <vyacheslav08071996@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vdyakov/nestjs-generator.git"
  },
  "bugs": {
    "url": "https://github.com/vdyakov/nestjs-generator/issues"
  },
  "homepage": "https://github.com/vdyakov/nestjs-generator#readme",
  "dependencies": {
    "chalk": "^5.2.0",
    "ejs": "^3.1.9",
    "minimist": "^1.2.8",
    "ora": "^6.3.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.2",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.15.11",
    "@types/prompts": "^2.4.4",
    "prettier": "^2.8.7",
    "copyfiles": "^2.4.1",
    "rimraf": "^4.4.1",
    "tsc-alias": "^1.8.5",
    "typescript": "^5.0.4"
  }
}
